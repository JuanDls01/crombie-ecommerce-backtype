{
   "compilerOptions": {
     "target": "es6",
     "baseUrl": ".",
     "module": "commonjs",
     "moduleResolution": "node",
     "outDir": "dist",
     "esModuleInterop": true,
     "forceConsistentCasingInFileNames": true,
     "strictPropertyInitialization": false,
     "strict": true,
     "skipLibCheck": true,
     "experimentalDecorators": true,
     "emitDecoratorMetadata": true,
     "resolveJsonModule": true,
     "allowJs": true,
     
   }
 }